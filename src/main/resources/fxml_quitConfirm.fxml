<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="340.0" prefWidth="550.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.QuitConfirmController">
   <children>
      <Label layoutX="31.0" layoutY="44.0" prefHeight="119.0" prefWidth="489.0" text="Are you sure you want to quit?">
         <font>
            <Font size="36.0" />
         </font>
      </Label>
      <ImageView fx:id="yesButton" fitHeight="95.0" fitWidth="183.0" layoutX="98.0" layoutY="168.0" onMouseClicked="#yesQuit" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@yesButton.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="95.0" fitWidth="156.0" layoutX="300.0" layoutY="168.0" onMouseClicked="#noQuit" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@noButton.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
