<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.MenuConfirmController">
   <children>
      <Label layoutX="26.0" layoutY="70.0" prefHeight="105.0" prefWidth="549.0" text="Are you sure want to go back to the menu?">
         <font>
            <Font size="28.0" />
         </font>
      </Label>
      <ImageView fx:id="yesButton" fitHeight="96.0" fitWidth="143.0" layoutX="97.0" layoutY="212.0" onMouseClicked="#yesMenu" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@yesButton.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="96.0" fitWidth="162.0" layoutX="326.0" layoutY="212.0" onMouseClicked="#noMenu" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@noButton.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
